<Window x:Class="BambuShootProject.WPF.CalibrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BambuShootProject.WPF"
        mc:Ignorable="d"
        Title="CalibrationWindow" Height="720" Width="1280">
    <Grid x:Name="Gridimage1" Margin="0,0,411,100">
        <Image Name="image1" Grid.Column="0" Height="317" HorizontalAlignment="Left" Margin="251,121,0,0"  Stretch="Fill" VerticalAlignment="Top" Width="331" >
        </Image>
        <Canvas x:Name="BackPanel" Margin="251,121,-231,-54">
            <Rectangle x:Name="selectionRectangle" Stroke="LightBlue" Fill="#220000FF" Visibility="Collapsed" />
        </Canvas>
        <Grid x:Name="other" Margin="231,54,-231,-54">
            <Button Content="Crop" Height="23" HorizontalAlignment="Left" Margin="372,207,0,0" Name="Go" VerticalAlignment="Top" Width="41" Click="Go_Click" FontWeight="Bold"  />
            <Image Height="317" HorizontalAlignment="Left" Margin="443,74,0,0" Name="image2" Stretch="Fill" VerticalAlignment="Top" Width="331" />
            <Button x:Name="button" Content="Next" HorizontalAlignment="Left" Margin="372,440,0,0" VerticalAlignment="Top" Width="75"  Click="button_Click"/>

        </Grid>
    </Grid>
</Window>
